# AutoApp Frontend - Environment Configuration Template
# Copy this file to .env and customize values for your environment

# =============================================================================
# API ENDPOINTS & BACKEND CONNECTION
# =============================================================================

# Backend API base URL
VITE_API_BASE_URL=http://localhost:8080
VITE_API_PREFIX=/api/v1

# WebRTC streaming endpoints
VITE_WEBRTC_SIGNALING_URL=ws://localhost:8080/webrtc
VITE_WEBRTC_STUN_SERVER=stun:stun.l.google.com:19302

# =============================================================================
# UI CONFIGURATION
# =============================================================================

# Application settings
VITE_APP_TITLE=AutoApp UI Map & Intelligent Flow Engine
VITE_APP_VERSION=1.0.0

# UI feature flags
VITE_ENABLE_DISCOVERY_PANEL=true
VITE_ENABLE_GRAPH_VISUALIZATION=true
VITE_ENABLE_FLOW_EDITOR=true
VITE_ENABLE_LLM_ASSISTANCE=true

# =============================================================================
# PERFORMANCE & TIMING
# =============================================================================

# UI timeouts and intervals
VITE_UI_CAPTURE_TIMEOUT=10000
VITE_GRAPH_RENDER_TIMEOUT=5000
VITE_FLOW_VALIDATION_TIMEOUT=3000

# Animation and interaction settings
VITE_ANIMATION_DURATION=300
VITE_TOOLTIP_DELAY=500
VITE_DEBOUNCE_DELAY=300

# =============================================================================
# DEVELOPMENT & DEBUGGING
# =============================================================================

# Development mode settings
VITE_DEV_MODE=false
VITE_DEBUG_MODE=false
VITE_ENABLE_CONSOLE_LOGS=false

# Mock data for development
VITE_USE_MOCK_DATA=false
VITE_MOCK_GRAPH_PATH=/mock-data/sample-graph.json
VITE_MOCK_FLOWS_PATH=/mock-data/sample-flows.json

# =============================================================================
# WEBRTC CONFIGURATION
# =============================================================================

# WebRTC connection settings
VITE_WEBRTC_TIMEOUT=30000
VITE_WEBRTC_RECONNECTION_ATTEMPTS=3
VITE_WEBRTC_ICE_TIMEOUT=10000

# WebRTC quality settings
VITE_WEBRTC_PREFERRED_RESOLUTION=720p
VITE_WEBRTC_MAX_FRAME_RATE=30
VITE_WEBRTC_MIN_BITRATE=500
VITE_WEBRTC_MAX_BITRATE=3000

# =============================================================================
# AUTHENTICATION & SECURITY
# =============================================================================

# Authentication settings (if enabled)
VITE_AUTH_ENABLED=false
VITE_AUTH_TOKEN_KEY=autoapp_token
VITE_SESSION_TIMEOUT=3600

# CORS settings
VITE_CORS_ENABLED=true
VITE_CORS_ORIGIN=*

# =============================================================================
# FEATURE FLAGS
# =============================================================================

# Experimental features
VITE_ENABLE_EXPERIMENTAL_FEATURES=false
VITE_ENABLE_BETA_GRAPH_LAYOUTS=false
VITE_ENABLE_ADVANCED_SELECTORS=false

# UI/UX features
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_KEYBOARD_SHORTCUTS=true
VITE_ENABLE_CONTEXT_HELP=true

# =============================================================================
# INTEGRATION SETTINGS
# =============================================================================

# Claude Code integration
VITE_CLAUDE_INTEGRATION_ENABLED=true
VITE_CLAUDE_API_BASE_URL=/api/claude
VITE_CLAUDE_REQUEST_TIMEOUT=30000

# External services
VITE_EXTERNAL_EMULATOR_URL=http://localhost:5555
VITE_ADB_PROXY_URL=ws://localhost:5556

# =============================================================================
# LOGGING & MONITORING
# =============================================================================

# Frontend logging
VITE_LOG_LEVEL=warn
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_PERFORMANCE_MONITORING=false

# User analytics
VITE_ENABLE_ANALYTICS=false
VITE_ANALYTICS_ID=

# =============================================================================
# LOCALIZATION
# =============================================================================

# Language and region
VITE_DEFAULT_LOCALE=en-US
VITE_SUPPORTED_LOCALES=en-US,es-ES,fr-FR,de-DE,ja-JP

# Timezone settings
VITE_DEFAULT_TIMEZONE=UTC
VITE_ENABLE_TIMEZONE_DETECTION=true

# =============================================================================
# BUILD & DEPLOYMENT
# =============================================================================

# Build configuration
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true
VITE_BUILD_TARGET=es2020

# Deployment settings
VITE_PUBLIC_URL=/
VITE_BASE_PATH=/
VITE_ASSET_CDN_URL=

# =============================================================================
# CACHE SETTINGS
# =============================================================================

# Browser caching
VITE_CACHE_BUSTING=true
VITE_SERVICE_WORKER_ENABLED=false

# API caching
VITE_API_CACHE_TTL=300000
VITE_GRAPH_CACHE_TTL=600000
VITE_ENABLE_OFFLINE_MODE=false